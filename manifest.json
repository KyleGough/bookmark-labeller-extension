{
  "manifest_version": 2,
  "name": "Bookmark Labeller",
  "version": "1.0.0",
  "description": "",
  "browser_action": {
    "default_title": "Bookmark Labeller",
    "default_popup": "src/index.html"
  },
  "background": {
    "scripts": ["src/background.js"]
  },
  "permissions": [
    "<all_urls>",
    "bookmarks",
    "menus",
    "storage",
    "tabs"
  ]
}
