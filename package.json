    "url": "https://github.com/KyleGough/bookmark-labeller-extension/issues"
  },
  "homepage": "https://github.com/KyleGough/bookmark-labeller-extension#readme",
  "devDependencies": {
    "@parcel/config-webextension": "^2.7.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "web-ext": "^7.3.1"
  }
